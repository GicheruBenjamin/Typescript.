{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "checkJs": true,
      "sourceMap": false,
      "inlineSourceMap": true,
      "inlineSources": true,
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules/**/*"]
  }